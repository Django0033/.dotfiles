// Inspired by Catnap
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small"
    },
    "display": {
        "separator": "",
        "key": {
            "width": 15
        }
    },
    "modules": [
        {
            "type": "title",
            "color": {
                "user":"green",
                "at":"red",
                "host":"blue"
              }
        },
        {
            "key": "󰅐 uptime",
            "type": "uptime",
            "keyColor": "33"
        },
        {
            "key": "{icon} distro",
            "type": "os",
            "keyColor": "34"
        },
        {
            "key": " term",
            "type": "terminal",
            "keyColor": "31"
        },
        {
            "key": "󰍛 cpu",
            "type": "cpu",
            "showPeCoreCount": true,
            "keyColor": "33"
        },
        {
            "key": "󰉉 disk",
            "type": "disk",
            "folders": "/",
            "keyColor": "34"
        },
        {
            "key": " memory",
            "type": "memory",
            "keyColor": "35"
        },
        {
            "key": " colors",
            "type": "colors",
            "symbol": "circle",
            "keyColor": "39"
        }
    ]
}
